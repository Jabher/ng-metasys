{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "interface-name": false,
    "max-line-length": [
      true,
      100
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-empty": false
  }
}
