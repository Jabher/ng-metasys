{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "interface-name": false,
    "max-line-length": [
      true,
      100
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ]
  }
}
