{
  "name": "ng-metasys",
  "version": "1.0.0-alpha.0",
  "description": "A decorator framework for AngularJS that makes easy using Angular meta-info with ES2015 modules",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run tsc && npm run remove:specs",
    "tsc": "tsc -p src ",
    "remove:specs": "rimraf \"dist/**/*.spec.js\" \"dist/**/*.spec.js.map\" \"dist/**/*.spec.d.ts\"",
    "prepublish": "npm run build",
    "test": "karma start config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lodin/ng-metasys.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "decorators",
    "metadata"
  ],
  "author": "Vlad Rindevich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lodin/ng-metasys/issues"
  },
  "homepage": "https://github.com/Lodin/ng-metasys#readme",
  "devDependencies": {
    "@types/angular": "^1.5.7",
    "@types/camelcase": "0.0.30",
    "@types/jasmine": "^2.5.41",
    "@types/reflect-metadata": "0.0.5",
    "awesome-typescript-loader": "^3.0.4-rc.0",
    "codecov": "^1.0.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.6",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.2.0-dev.20170210",
    "webpack": "^2.2.1"
  },
  "optionalDependencies": {
    "core-js": "^2.4.1"
  },
  "peerDependencies": {
    "angular": "^1.5.0",
    "tslib": "^1.5.0"
  },
  "dependencies": {
    "camelcase": "^4.0.0"
  }
}
